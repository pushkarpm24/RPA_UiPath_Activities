                                          </ui:SendMail.Files>
                                        </ui:SendMail>
                                      </If.Then>
                                    </If>
                                  </If.Else>
                                </If>
                              </If.Else>
                            </If>
                          </Sequence>
                        </ActivityAction>
                      </ui:ForEach.Body>
                    </ui:ForEach>
